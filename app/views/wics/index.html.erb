<div class="page-header">
  <h1>Wics</h1>
</div>

<p><a href="/wics/new" class="btn btn-primary">Add a New Wic</a></p>

<table class="table table-hover">
  <thead>
    <tr>
      <th>Wic household size</th>
      <th>Wic gross income</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @wics.each do |wic| %>
      <tr>
        <td><%= wic.wic_household_size %></td>
        <td><%= wic.wic_gross_income %></td>
        <td>
          <a href="/wics/<%= wic.id %>" class="btn btn-primary">
            <i class="fa fa-search-plus"></i>
          </a>
        </td>
        <td>
          <a href="/wics/<%= wic.id %>/edit" class="btn btn-warning">
            <i class="fa fa-edit"></i>
          </a>
        </td>
        <td>
          <a href="/delete_wic/<%= wic.id %>" class="btn btn-danger">
            <i class="fa fa-trash-o"></i>
          </a>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

