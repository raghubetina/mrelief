<div class="page-header">
  <h1>Auto repair assistances</h1>
</div>

<p><a href="/auto_repair_assistances/new" class="btn btn-primary">Add a New Auto Repair Assistance</a></p>

<table class="table table-hover">
  <thead>
    <tr>
      <th>Auto household size</th>
      <th>Auto gross income</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @auto_repair_assistances.each do |auto_repair_assistance| %>
      <tr>
        <td><%= auto_repair_assistance.auto_household_size %></td>
        <td><%= auto_repair_assistance.auto_gross_income %></td>
        <td>
          <a href="/auto_repair_assistances/<%= auto_repair_assistance.id %>" class="btn btn-primary">
            <i class="fa fa-search-plus"></i>
          </a>
        </td>
        <td>
          <a href="/auto_repair_assistances/<%= auto_repair_assistance.id %>/edit" class="btn btn-warning">
            <i class="fa fa-edit"></i>
          </a>
        </td>
        <td>
          <a href="/delete_auto_repair_assistance/<%= auto_repair_assistance.id %>" class="btn btn-danger">
            <i class="fa fa-trash-o"></i>
          </a>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

