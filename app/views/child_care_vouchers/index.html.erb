<div class="page-header">
  <h1>Child care vouchers</h1>
</div>

<p><a href="/child_care_vouchers/new" class="btn btn-primary">Add a New Child Care Voucher</a></p>

<table class="table table-hover">
  <thead>
    <tr>
      <th>Ccdf dependent no</th>
      <th>Cccdf gross income</th>
      <th>Ccdf eligible children</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @child_care_vouchers.each do |child_care_voucher| %>
      <tr>
        <td><%= child_care_voucher.ccdf_dependent_no %></td>
        <td><%= child_care_voucher.cccdf_gross_income %></td>
        <td><%= child_care_voucher.ccdf_eligible_children %></td>
        <td>
          <a href="/child_care_vouchers/<%= child_care_voucher.id %>" class="btn btn-primary">
            <i class="fa fa-search-plus"></i>
          </a>
        </td>
        <td>
          <a href="/child_care_vouchers/<%= child_care_voucher.id %>/edit" class="btn btn-warning">
            <i class="fa fa-edit"></i>
          </a>
        </td>
        <td>
          <a href="/delete_child_care_voucher/<%= child_care_voucher.id %>" class="btn btn-danger">
            <i class="fa fa-trash-o"></i>
          </a>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

