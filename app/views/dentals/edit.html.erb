<div class="page-header">
  <h1>Edit Dental #<%= @dental.id %></h1>
</div>

<% if @dental.errors.any? %>
  <% @dental.errors.full_messages.each do |message| %>
    <div class="alert alert-dismissable alert-danger">
      <button type="button" class="close" data-dismiss="alert" aria-hidden="true">&times;</button>
      <%= message %>
    </div>
  <% end %>
<% end %>

<form action="/update_dental/<%= @dental.id %>" class="form-horizontal">
  <!-- Label and input for dental_gross_income -->
  <div class="form-group">
    <label for="dental_gross_income" class="col-sm-2 control-label">
      Dental gross income
    </label>
    <div class="col-sm-10">
      <input type="text" id="dental_gross_income" name="dental_gross_income" placeholder="Dental gross income" class="form-control" value="<%= @dental.dental_gross_income %>">
    </div>
  </div>

  <!-- Label and input for dental_household_size -->
  <div class="form-group">
    <label for="dental_household_size" class="col-sm-2 control-label">
      Dental household size
    </label>
    <div class="col-sm-10">
      <input type="text" id="dental_household_size" name="dental_household_size" placeholder="Dental household size" class="form-control" value="<%= @dental.dental_household_size %>">
    </div>
  </div>

  <div class="form-group">
    <div class="col-sm-offset-2 col-sm-10">
      <input type="submit" value="Update Dental" class="btn btn-primary">
    </div>
  </div>
</form>
